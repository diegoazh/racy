{
  "name": "@racy/azure-middleware",
  "version": "1.1.1",
  "description": "racy implementation for azure middleware inspired on Middy",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:build": "rimraf dist",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "lint": "prettier src --write && eslint \"src/**/*.{ts,js}\" --fix",
    "build": "npm run clean:build && tsc --project tsconfig.build.json",
    "release": "npm run build && cp package.json ./dist && cp README.md ./dist && cp LICENSE.md ./dist && cd ./dist && npm publish --access public && cd .."
  },
  "keywords": [
    "serverless",
    "functions",
    "azure",
    "middleware"
  ],
  "author": "Diego A. Zapata HÃ¤ntsch",
  "license": "MIT",
  "dependencies": {
    "@azure/functions": "^1.2.3",
    "@types/node": "^16.9.2"
  },
  "devDependencies": {
    "@jest/types": "^27.1.1",
    "@types/azure-function-log-intercept": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
